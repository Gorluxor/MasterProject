  <specificPropertyOf rdf:resource="http://purl.org/dc/terms/relation"/>
  <rdfs:comment xml:lang="en">Inverse of "basis_for". Indicates that this legal resource is empowered by another one, typically a constitution, a treaty or an enabling act.</rdfs:comment>
  <rdfs:label xml:lang="en">based on</rdfs:label>
  <owl:versionInfo>created in v1.0</owl:versionInfo>
  <owl:versionInfo xml:lang="en">modified in v1.1</owl:versionInfo>
  <core:historyNote xml:lang="en">v1.1 : improved definition</core:historyNote>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#basis_for">
  <rdfs:domain>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#LegalExpression"/>
